{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "width": 500,
  "height": 300,
  "data": {
    "url": "world-110m.json",
    "format": {"type": "topojson", "feature": "counties"}
  },
  "transform": [
    {
      "lookup": "id",
      "from": {
        "data": {"url": "threatenedspecies.csv"},
        "key": "id",
        "fields": ["rate"]
      }
    }
  ],
  "projection": {"type": "albersUsa"},
  "mark": "geoshape",
  "encoding": {"color": {"field": "rate", "type": "quantitative"}}
}